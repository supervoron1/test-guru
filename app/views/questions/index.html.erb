<h3>Questions</h3>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @questions.each do |question| %>
    <tr>
      <td><%= question.id %></td>
      <td><%= question.body %></td>
      <td><%= link_to 'Show', test_question_path(@test, question) %></td>
      <td><%= link_to 'Delete', test_question_path(@test, question), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'Back to test page', test_path(@test.id) %>
